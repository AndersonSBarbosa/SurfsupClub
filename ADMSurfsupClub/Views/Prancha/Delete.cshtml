@model Entidades.PranchaImagem
@{
    ViewBag.Title = "Excluir Imagem";
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Delete</title>
</head>
@using (Html.BeginForm())
{
<body>
    <div> 
        @Html.EditorFor(model => model.IdImagem, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
        @Html.EditorFor(model => model.IdPrancha, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
        @Html.EditorFor(model => model.Imagem, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })

        <img src="~/Content/ImagensPrancha/@Model.Imagem" />
        <input class="btn btn-primary" type="submit" name="btnguardar" value="Excluir" />
    </div>
</body>
}
</html>
